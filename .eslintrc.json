{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "curly": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-return-assign": "off",
    "multiline-ternary": "off",
    "no-use-before-define": "off"
  }
}
